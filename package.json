{
  "name": "hc-ts-template",
  "version": "0.1.0",
  "description": "",
  "proxy": "http://localhost:4141",
  "private": true,
  "scripts": {
    "hc:start-web": "npm run hc:build && echo 'Starting Holochain Dev Server process...' && cd build && ../node_modules/pm2/bin/pm2 start ../ecosystem.config.js --watch",
    "hc:build-common": "tsc -p dna-src && cpx \"dna-src/**/*.json\" build/dna",
    "hc:build": "rm -r build/dna; npm run hc:build-common",
    "hc:build-win": "rm -r build/dna & npm run hc:build-common",
    "hc:dev": "npm run hc:build && cd build && ../hc-bin/hcdev web",
    "dev:start": "npm run hc:start-web && npm run dev:watch && npm run start",
    "dev:watch": "./node_modules/nodemon/bin/nodemon.js --config ./nodemon.json &",
    "hc:win": "npm run hc:build-win && cd build && ../hc-bin/hcdev web",
    "hc:test": "npm run hc:build && cp -r ./test/ ./build/ && cd build && ../hc-bin/hcdev test",
    "ui:build": "rm -r build/ui; node scripts/build.js && cp -r ui-src/build/ build/ui/",
    "start": "node scripts/start.js",
    "build": "mkdir -p build/ui build/dna; npm run hc:build && npm run ui:build",
    "test": "node scripts/test.js --env=jsdom",
    "gulp-stuff": "./node_modules/.bin/gulp",
    "hc:process": "./node_modules/pm2/bin/pm2 list",
    "hc:log": "./node_modules/pm2/bin/pm2 logs 'Holochain Web Server'",
    "hc:stop": "./node_modules/pm2/bin/pm2 stop 'Holochain Web Server' --watch",
    "hc:remove-process": "./node_modules/pm2/bin/pm2 delete 'Holochain Web Server'",
    "hc:restart-web": "npm run hc:build && cd build && ../node_modules/pm2/bin/pm2 restart 'Holochain Web Server' && echo 'Reloaded Holochain Dev Server process...'"
  },
  "author": "",
  "license": "GPL",
  "dependencies": {
    "autoprefixer": "7.1.6",
    "babel-jest": "^22.1.0",
    "babel-loader": "^7.1.2",
    "babel-preset-react-app": "^3.1.1",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "css-loader": "0.28.7",
    "dotenv": "4.0.0",
    "dotenv-expand": "4.2.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "0.11.2",
    "fork-ts-checker-webpack-plugin": "^0.2.8",
    "fs-extra": "3.0.1",
    "html-webpack-plugin": "2.29.0",
    "immutable": "^3.8.2",
    "jest": "22.4.2",
    "object-assign": "4.1.1",
    "pm2": "^3.1.2",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.8",
    "promise": "8.0.1",
    "raf": "3.4.0",
    "react": "^16.4.2",
    "react-dev-utils": "^5.0.1",
    "react-dom": "^16.3.2",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "react-virtualized": "^9.20.1",
    "redux": "^4.0.0",
    "redux-immutable": "^4.0.0",
    "resolve": "1.6.0",
    "source-map-loader": "^0.2.1",
    "style-loader": "0.19.0",
    "sw-precache-webpack-plugin": "0.11.4",
    "ts-jest": "22.0.1",
    "ts-loader": "^2.3.7",
    "tsconfig-paths-webpack-plugin": "^2.0.0",
    "tslint": "^5.7.0",
    "tslint-config-prettier": "^1.10.0",
    "tslint-react": "^3.2.0",
    "uglifyjs-webpack-plugin": "^1.1.8",
    "url-loader": "0.6.2",
    "webpack": "3.8.1",
    "webpack-dev-server": "2.9.4",
    "webpack-manifest-plugin": "1.3.2",
    "whatwg-fetch": "2.0.3"
  },
  "devDependencies": {
    "@types/jest": "^23.3.1",
    "@types/node": "^10.5.4",
    "@types/react": "^16.4.8",
    "cpx": "^1.5.0",
    "gulp": "^3.9.1",
    "gulp-cli": "^2.0.1",
    "gulp-exec": "^3.0.2",
    "gulp-typescript": "^5.0.0-alpha.3",
    "immutable": "^3.8.2",
    "nodemon": "^1.18.4",
    "typescript": "^2.9.2"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/ui-src/src/**/__tests__/**/*.(j|t)s?(x)",
      "<rootDir>/ui-src/src/**/?(*.)(spec|test).(j|t)s?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.tsx?$": "<rootDir>/config/jest/typescriptTransform.js",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs|ts|tsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "web.js",
      "js",
      "web.jsx",
      "jsx",
      "json",
      "node",
      "mjs"
    ],
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.test.json"
      }
    }
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
